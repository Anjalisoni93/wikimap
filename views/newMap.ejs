<%- include('partials/_header') %>

<div class="newmap-form">
  <form action = "/maps" method="POST">
    <div class="form-icon">
        <span><i class="bi bi-map"></i>
        </span>
    </div>
    <div class="form-group">
        <!-- <input type="text" class="form-control item" id="country" placeholder="Country"> -->

        <select class="form-control item" name = "Country">
          <option value="Canada">Canada</option>
          <option value="USA">United States</option>
          <option value="India">India</option>
        </select>
    </div>
    <div class="form-group">
        <input type="city" name = "City" class="form-control item" id="city" placeholder="City">
    </div>
    <div class="form-group">
        <input type="text" name = "title" class="form-control item" id="title" placeholder="Title">
    </div>
    <div class="form-group">
      <button type="submit" class="btn btn-block submit">Submit</button>
    </div>
  </form>
</div>

<%- include('partials/_footer') %>
