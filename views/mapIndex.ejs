<%- include('partials/_header') %>
<main style="margin: 1em;">
  <h3 class = "text-center">All Maps</h3>
  <div class="mapList">
    <table class="table table-borderless">
    <thead>
      <tr>
        <th scope="col">List Of Maps</th>
        <th scope="col">Craeted By</th>
        </tr>
    </thead>
    <tbody>
      <% for(let map of maps) { %>
        <tr>
          <td><a href="/maps/<%=map.id %>" > <%=map.map%></a></td>
          <td><%=map.created_by%></td>
        </tr>
      <% } %>
    </tbody>
  </table>
  </div>
</main>
<%- include('partials/_footer') %>

